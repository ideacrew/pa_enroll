@import "variables";
.blue {
  color: #29578D;
}
.darkblue {
  color: #003260;
}
.red {
  color: red;
}
.lg {
  color: #7b7b7b;
}
.dg {
  color: #414141;
}
.ttu {
  text-transform: uppercase;
}
.ttc {
  text-transform: capitalize;
}
.nine {
  font-size: 9px;
}
.twelve {
  font-size: 12px;
}
.fourteen {
  font-size: 14px;
}
.wsn {
  white-space: nowrap;
}
.di {
  display: inline;
}
.tu {
  &:hover {
    text-decoration: underline;
  }
}
